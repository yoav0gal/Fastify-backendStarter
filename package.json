{
  "name": "alpha-backend-template",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "types": "index.d.ts",
  "scripts": {
    "test": "yarn build && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "yarn build && fastify start -l info -L dist/configs/loggerConfigurations.js  dist/src/app.js",
    "build": "rimraf dist && tsc && tsc-alias",
    "watch": "tsc && (concurrently \"tsc -w  --preserveWatchOutput --pretty\" \"tsc-alias -w\")",
    "format": "prettier --write \"src/**/*.ts\"",
    "genController": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./configs/codeBasedTools/scripts/generateController/generateController.ps1",
    "predev": "yarn build",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"yarn watch\"  \"@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./configs/codeBasedTools/scripts/runDev.ps1\""
  },
  "keywords": [],
  "author": "Yoav Gal",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/error": "^3.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.2.1",
    "@fastify/swagger-ui": "^1.3.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@sinclair/typebox": "^0.25.21",
    "axios": "^1.2.3",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "fastify-cli": "^5.7.0",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^4.4.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0"
  },
  "devDependencies": {
    "@fastify/cors": "^8.2.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/fastify-jwt": "^0.8.1",
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "tap": "^16.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
